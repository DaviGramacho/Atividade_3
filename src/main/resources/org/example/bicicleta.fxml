<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="580.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.BicicletaController">
   <children>
      <Label layoutX="112.0" layoutY="24.0" text="Bem-Vindo a sua Bicicleta">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Button fx:id="btn_enviar_info_bicicleta" layoutX="272.0" layoutY="277.0" mnemonicParsing="false" onAction="#enviarInfoCarro" text="ENVIAR" />
      <Label layoutX="84.0" layoutY="116.0" text="Marca da Bicicleta:">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Label>
      <Label layoutX="84.0" layoutY="172.0" text="Velocidade atual:">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Label>
      <Label layoutX="84.0" layoutY="218.0" text="Marcha atual:">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="marcaBicicletaTextField" layoutX="225.0" layoutY="114.0" prefHeight="26.0" prefWidth="157.0" promptText="Digite a marca da bicicleta" />
      <TextField fx:id="velocidadeAtualBicicletaTextField" layoutX="212.0" layoutY="170.0" prefHeight="26.0" prefWidth="285.0" promptText="Digite a velocidade atual (apenas numeros inteiros)" />
      <TextField fx:id="marchaAtualBicicletaTextField" layoutX="190.0" layoutY="216.0" prefHeight="26.0" prefWidth="266.0" promptText="Digite a marcha atual (apenas numeros inteiros)" />
      <TableView fx:id="tblView" layoutX="187.0" layoutY="352.0" prefHeight="200.0" prefWidth="227.0">
        <columns>
          <TableColumn fx:id="tblMarca" prefWidth="75.0" text="Marca" />
          <TableColumn fx:id="tblVel" prefWidth="75.0" text="Velocidade" />
            <TableColumn fx:id="tblMarcha" prefWidth="75.0" text="Marcha" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
